{
    "manifest_version" : 2,
    "name" : "IOTA Hackathon",
    "version" : "0.001",
    "content_scripts" : [
        {
            "matches" : [
                "<all_urls>"
            ],
            "js" : ["content.js"]
        }
    ],
    "browser_action":{
        "default_icon" : "tictactoe.png",
        "default_popup" : "Form/index.html",
        "default_title" : "IOTA"
    },
    "permissions" : [
        "tabs",
        "storage"
    ]

}